node { 
    def mavenhome = tool name: 'mavenhome', type: 'maven'
    stage('Git-Checkout') { 
       git branch: 'main', credentialsId: 'git-credential', url: 'https://github.com/EricNk/web-app.git'
    }
    stage('Clean-Compile') { 
       $(mavenhome)/bin/mvn clean compile
    }
    stage('Package') { 
       $(mavenhome)/bin/mvn package
    }
}
