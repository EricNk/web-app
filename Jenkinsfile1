pipeline { 
    agent any 
    tools {
        maven 'mavenhome'
        }
    stages {
        stage('SCM-Checkout') { 
            steps { 
                git branch: 'main', credentialsId: 'git-credential', url: 'https://github.com/EricNk/web-app.git' 
            }
        }
        stage('Clean-Compile'){
            steps {
                sh "mvn clean compile"
            }
        }
        stage('Package') {
            steps {
                sh "mvn package"
            }
        }
    }
